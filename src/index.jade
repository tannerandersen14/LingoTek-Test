    script(src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.10/angular-ui-router.js")
    script(src="https://cdnjs.cloudflare.com/ajax/libs/ngStorage/0.3.10/ngStorage.js")
    script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.2/jquery.min.js")
